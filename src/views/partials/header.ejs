  <title><%= (req.locale!="en")?(title+` | In ${req.language}`):title %></title>
  <style>
body{display:flex;flex-direction:column;}#loading{margin:0;padding:0;background-color:#36393F;height:100%;width:100%;position:fixed;z-index:9999;margin-top:0;top:0}#loading-center{width:100%;height:100%;position:relative}#loading-center-absolute{position:absolute;left:50%;top:50%;height:200px;width:200px;margin-top:-100px;margin-left:-100px}.loading{position:fixed;text-align:center;padding:10px;background-color:<%= bgcolor %>}#text1{font-style:italic;color:#fff;font-family:Roboto,sans-serif;font-size:25px;z-index:100;top:68%;width:100%}#object{top:25%;left:42%}body{background-color:<%= bgcolor %>}.squares{position:absolute;left:0;right:0;top:0;bottom:0;margin:auto;width:50px;height:50px;animation:scale ease 1.4s infinite}.squares::before{content:'';position:absolute;width:15px;height:15px;background:<%= hex %>;top:-10px;left:-10px;animation:top ease 3s infinite}.squares::after{content:'';position:absolute;width:15px;height:15px;background:<%= hex %>;bottom:-10px;right:-10px;animation:bottom ease 3s infinite}@keyframes top{0%{top:-10px}20%{top:100%;left:-10px}40%{left:100%;top:100%;transform:rotate(180deg)}60%{top:-10px;left:100%}80%{top:-10px;transform:rotate(360deg)}100%{left:-10px}}@keyframes bottom{0%{bottom:-10px}20%{bottom:100%;right:-10px}40%{right:100%;bottom:100%;transform:rotate(180deg)}60%{bottom:-10px;right:100%}80%{bottom:-10px;transform:rotate(360deg)}100%{right:-10px}}@keyframes scale{0%{transform:scale(1)}50%{transform:scale(.7)}100%{transform:scale(1)}}#loadtext{font-style:italic;text-align:center;color:#fff;position:absolute;font-family:Roboto,sans-serif;font-size:15px;top:68%;width:100%;text-align:center}img.emoji{height:1em;width:1em;margin:0 0.05em 0 0.1em;vertical-align:-0.229em;}img.emoji-double{height:2em;width:2em;margin:0 0.05em 0 0.1em;vertical-align:-0.229em;}img.logo{height:1em;width:1em;vertical-align:-0.15em;}
::-webkit-scrollbar {
  width: 20px;
}
::-webkit-scrollbar-track {
  box-shadow: inset 0 0 5px <%= bgcolor %>; 
  border-radius: 10px;
}
::-webkit-scrollbar-thumb {
  background: <%= hex %>; 
  border-radius: 10px;
}
  </style>
  <script>
  var user = <%- (user)?JSON.stringify(user):"null" %>
  var $ = function(r){return document.querySelector(r)};
  var $$ = function(r){return Array.from(document.querySelectorAll(r))};
</script>
<script async src="https://www.googletagmanager.com/gtag/js?id=G-GQV88WJSP1"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  var pattern = ['ArrowUp', 'ArrowUp', 'ArrowDown', 'ArrowDown', 'ArrowLeft', 'ArrowRight', 'ArrowLeft', 'ArrowRight', 'b', 'a'];
  var current = 0;
  var keyHandler = function(event) {
    if (pattern.indexOf(event.key) < 0 || event.key !== pattern[current]) {
      current = 0;
      return;
    }
    current++;
    if (pattern.length === current) {
      current = 0;
      window.alert('RDL\'s easter egg! You found it!');
    }
  };
  document.addEventListener('keydown', keyHandler, false);
  gtag('js', new Date());
  gtag('config', 'G-GQV88WJSP1');
</script>
